{
  "name": "estimator",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:test:emulator": "ng serve --configuration test-emulator",
    "start:test:emulator:ssl": "ng serve --configuration test-emulator --ssl",
    "start:test": "ng serve --configuration test",
